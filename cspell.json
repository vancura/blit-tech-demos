{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "language": "en",
  "allowCompoundWords": true,
  "dictionaries": ["typescript", "node", "html", "css", "fonts", "npm", "softwareTerms"],
  "words": [
    "Ambilab",
    "bindgroup",
    "bindgroups",
    "biomejs",
    "blit",
    "Bresenham",
    "BTAPI",
    "btfont",
    "cfignore",
    "commitlint",
    "cooldown",
    "cooldowns",
    "esbuild",
    "Fabrizio",
    "gamepad",
    "invinc",
    "nsis",
    "pnpm",
    "rects",
    "retroblit",
    "RGBA",
    "Rollup",
    "Schiavi",
    "spritesheet",
    "subsystem",
    "subsystems",
    "tilemap",
    "timestep",
    "Vančura",
    "Vite",
    "Vitest",
    "Václav",
    "WASD",
    "WebGPU",
    "webstorm",
    "yoffset"
  ],
  "flagWords": ["cant", "dont", "hte", "teh", "wont"],
  "ignorePaths": [
    "*.map",
    "*.min.js",
    ".git",
    ".pnpm-store",
    "CHANGELOG.md",
    "CLAUDE.md",
    "coverage",
    "dist",
    "node_modules",
    "pnpm-lock.yaml"
  ],
  "ignoreRegExpList": ["/#[0-9A-Fa-f]{3,8}/", "/0x[0-9A-Fa-f]+/", "/@[a-zA-Z0-9_-]+\\/[a-zA-Z0-9_-]+/"],
  "overrides": [
    {
      "filename": "**/*.ts",
      "words": ["instanceof", "keyof", "readonly", "typeof"]
    }
  ]
}
